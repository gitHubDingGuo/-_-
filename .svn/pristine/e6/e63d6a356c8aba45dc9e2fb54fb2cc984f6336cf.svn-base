.dialog {
	position: absolute;
	box-shadow: none;
	overflow: hidden;
	border: 0px none;
}

.dialog .dialog-min-header,
.dialog .dialog-header {
	background-color: rgb(247, 250, 255);
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom: 1px solid rgb(196, 219, 254);
	height: 44px;
	line-height: 44px;
	font-weight: 200;
}

.dialog .dialog-min-header h3,
.dialog .dialog-header h3 {
	color: rgb(66, 78, 103);
}

.dialog .dialog-min-header .select-text,
.dialog .dialog-header .select-text {
	-moz-user-select: initial;
}

.dialog .dialog-header-title em {
	font-style: normal;
	cursor: text;
	font-weight: 200;
}

.dialog .dialog-footer {
	padding-bottom: 15px;
}

.dialog .dialog-footer::after {
	content: ".";
	display: block;
	visibility: hidden;
	height: 0px;
	line-height: 0;
	font-size: 0px;
	clear: both;
}

.dialog .dialog-control {
	top: 13px;
	right: 12px;
}

.dialog .dialog-control .icon {
	font-size: 12px;
	text-align: center;
	line-height: 20px;
	color: rgb(95, 112, 148);
	margin: 0px 2px;
}

.dialog .dialog-control .icon .sicon {
	display: none;
}

.dialog .dialog-control .icon span {
	font-size: 14px;
}

.dialog .dialog-control .icon:hover {
	color: rgb(124, 138, 168);
}

.dialog .dialog-icon {
	font-size: 24px;
}



/*      分享模态框页面设计                 */

#shareContext .context {
	margin: 20px 0;
    padding: 10px 20px 0;
    height: 42px;
    line-height: 24px;
}

.share-success {
	color: #3b8cff;
    font-size: 14px;
    position: relative;
    padding-left: 26px;
    display: block;
}

.success-context {
	width: 530px;
    overflow: visible;
    z-index: 52;
    top: 125px;
    bottom: auto;
    left: 418px;
    right: auto;
    display: block;
    visibility: visible;
}

.share-success .icon {
	background: url(/static/img/share-ok.png) center no-repeat;
    position: absolute;
    left: 0;
    _left: -40px;
    top: -4px;
    _top: -8px;
    display: block;
    width: 17px;
    height: 31px;
}

.success-context .url .share-url-border {
	width: 380px;
    border: 1px solid #e9e9e9;
    border-radius: 4px;
    display: inline-block;
}

.url-info .copy-button-section {
	position: absolute;
	top: 0;
    right: 0;
}

.url-info {
	margin: 20px 0;
    position: relative;
    padding: 10px 20px 0;
    height: 120px;
    line-height: 24px;
}
.url {
	position: relative;
	width: 380px;
}
.share-url-border .share-url {
	width: 380px;
	height: 19px;
    line-height: 18px;
    padding: 8px;
    border: 1px solid #e9e9e9;
    border-radius: 4px;
    display: inline-block
}
.url-info .url-context {
	display: block!important;
	margin: 30px 0 16px;
    position: relative;
}