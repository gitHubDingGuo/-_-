* {
	margin: 0px;
	padding: 0px;
}

body {
	font-size: 14px;
	color: rgb(51, 51, 51);
	font-family: "Microsoft Yahei";
}

ul,
li {
	list-style-type: none;
}

#treeView {
	width: 600px;
	margin: 20px auto 0px;
	border: 1px solid rgb(242, 242, 242);
}

.treeNode {
	height: 32px;
	border-style: solid;
	border-color: rgb(255, 255, 255);
	-moz-border-top-colors: none;
	-moz-border-right-colors: none;
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	border-image: none;
	border-width: 1px 0px;
	cursor: pointer;
	font-size: 0px;
}

.treeNode:hover {
	background: rgb(244, 249, 253) none repeat scroll 0% 0%;
	border-color: rgb(229, 240, 251);
}

.treeNode-cur,
.treeNode-cur:hover {
	background: rgb(229, 240, 251) none repeat scroll 0% 0%;
	border-color: rgb(187, 212, 239) rgb(255, 255, 255);
}

.icon1 {
	display: inline-block;
	background: transparent url('../img/icon-tree_48f78c0.png') no-repeat scroll 0% 0%;
	vertical-align: middle;
}

.icon-minus {
	width: 15px;
	height: 15px;
	margin: 0px 4px;
	background-position: -30px -22px;
}

.icon-add {
	width: 15px;
	height: 15px;
	margin: 0px 4px;
	background-position: -30px 0px;
}

.icon-file {
	width: 22px;
	height: 18px;
	margin-right: 5px;
	background-position: 0px -22px;
}

.title {
	position: relative;
	top: 6px;
	font-size: 14px;
}

.treeNode-empty .icon-add {
	background-position: -999px -999px;
}

#treeView ul.none {
	display: none;
}