package com.netdisc.pojo;

import java.util.Date;

import lombok.Data;

/**
 * @author Stodger
 * @date 2019年8月28日
 * @version
 */
@Data
public class Share {
    /**
     * 分享Id(自增)
     */
    private Integer id;

    /**
     * 用户Id
     */
    private String userId;

    /**
     * 有效天数
     */
    private Integer effectiveTime;

    /**
     * 分享时间
     */
    private Date shareTime;

    /**
     * 分享验证码(私有分享)
     */
    private String shareCode;

    /**
     * 分享校验码
     */
    private String checkCode;

    /**
     * 分享的文件路径
     */
    private String content;
}